{
  "name": "evmoswap-protocol",
  "description": "EvmoSwap.org build on Evmos Chain",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npx hardhat compile",
    "flatten": "rm -rf flatten && npx waffle flatten",
    "mainnet:run": "npx hardhat run --network mainnet",
    "mainnet:deploy": "npx hardhat deploy --network mainnet",
    "mainnet:verify": "npx hardhat etherscan-verify --network mainnet",
    "bsctest:run": "npx hardhat run --network bsctest",
    "bsctest:deploy": "npx hardhat deploy --network bsctest",
    "bsctest:verify": "npx hardhat etherscan-verify --network bsctest",
    "testnet:run": "npx hardhat run --network testnet",
    "testnet:deploy": "npx hardhat deploy --network testnet",
    "testnet:verify": "npx hardhat etherscan-verify --network testnet"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.3",
    "chai": "^4.3.4",
    "commander": "^6.1.0",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.7",
    "hardhat": "^2.6.4",
    "hardhat-deploy": "^0.9.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.4.1",
    "@openzeppelin/contracts-upgradeable": "^3.4.1",
    "date-fns": "^2.25.0",
    "hardhat-deploy-ethers": "^0.3.0-beta.7",
    "hardhat-spdx-license-identifier": "^2.0.3"
  }
}
